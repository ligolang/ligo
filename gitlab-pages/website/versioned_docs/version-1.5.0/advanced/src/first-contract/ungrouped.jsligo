
type storage = int;
type result = [list<operation>, storage];

@entry
const increment = (n: int, store: storage): result =>
  [list([]), store + n];

@entry
const decrement = (n: int, store: storage): result =>
  [list([]), store - n];

[@view]
const v1 = (n : int, store : storage) : int => store + n