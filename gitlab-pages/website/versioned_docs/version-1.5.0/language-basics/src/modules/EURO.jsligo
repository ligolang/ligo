namespace EURO {
  export type t = nat;
  export const add = (a: t, b: t) : t => a + b;
  export const zero: t = 0n;
  export const one: t = 1n
}
type storage = EURO.t;

@entry
let main = (_action: unit, store: storage): [list<operation>, storage] =>
  [list([]), EURO.add (store, EURO.one)];
import US_DOLLAR = EURO;