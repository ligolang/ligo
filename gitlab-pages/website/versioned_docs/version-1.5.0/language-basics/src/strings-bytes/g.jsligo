/* Bitwise and */
const b_and           =  0x0005 & 0x0106; // 0x0004

/* Bitwise or */
const b_or            = 0x0005 | 0x0106; // 0x0107

/* Bitwise xor */
const b_xor           = 0x0005 ^ 0x0106; // 0x0103

/* Bitwise shift left */
const b_shift_left    = 0x06 << 8n; // 0x0600

/* Bitwise shift right */
const b_shift_right   = 0x0006 >> 1n; // 0x0003