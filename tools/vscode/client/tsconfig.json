{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "rootDir": "src",
        "sourceMap": true,
        "esModuleInterop": true,
        "noImplicitOverride": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true
    },
    "include": ["src", "commands/ligoCommands.ts"],
    "exclude": ["node_modules", ".vscode-test"]
}
