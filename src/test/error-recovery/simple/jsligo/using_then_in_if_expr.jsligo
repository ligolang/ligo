type storage = int;

type parameter = list<int>;

type return_ = [list<operation>, storage];

const hd = (x : list<int>) : int => {
  if (List.length(x) == 0n) then {
    return -1;
  } else {
    return hd(x);
  };
};

const main = ([a, b] : [parameter, storage]) : return_ =>
  [list([]) as list<operation>, (hd(a) + (b + 8) * 11)]
