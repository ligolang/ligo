type storage = int;

type parameter =
| ["Add", int]
| ["Sub", int];

type return_ = [list <operation>, storage];

const main = ([action, store]: [parameter, storage]) : return_ => {
  let store2 = store + match (action) {
    when (Add(n)) : n;
    when (Sub(n)) : -n
  };
  return [(list([]) as list <operation>), store2];
};
