type action_t =
@layout("comb")
    | ["A"]
    | ["B", int]
    | ["C", [int, int]];

type storage_t = 
@layout("comb")
{
    x: int,
    y: int,
    z: int
};

type return_t = [list<operation>, storage_t];
   
@entry
const main = (action: action_t, _: storage_t): return_t =>
    [
        list([]),
        match(action) {
            when(A()): { x: 10, y: 10, z: 10 };
            when(B(_)): { x: 20, y: 20, z: 20 };
            when(C(_)): { x: 20, y: 20, z: 20 };
        }
    ];
