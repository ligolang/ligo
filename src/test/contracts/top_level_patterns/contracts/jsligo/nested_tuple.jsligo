  = [[4 as nat, 4, "L"], [5 as nat, 5, "O"], [6 as nat, 6, "World"]]

type storage = [nat, int, string]

const main = (_ : unit, _ : storage) : [list<operation>, storage]
  => [list([]), [a1 + b1 + c1 + a4 + b4 + c4, 
         a2 + b2 + c2 + a5 + b5 + c5, 
         a3 + b3 + c3 + a6 + b6 + c6]]