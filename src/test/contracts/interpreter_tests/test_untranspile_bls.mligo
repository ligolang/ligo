module Test = Test.Next
let assert = Assert.assert

let test_fr =
  let bls = (0x1a0111ea397fe69a4b1ba7b6434bacd764774b84f38512bf6730d2a0f6b0f624 : bls12_381_fr) in
  let value = Test.Michelson.eval bls in
  let dec = (Test.Michelson.decompile value : bls12_381_fr) in
  assert (Test.String.show dec = Test.String.show bls)

