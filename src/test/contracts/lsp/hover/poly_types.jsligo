type func<a, b> = ["Func", (x : a) => b]

const incr = Func(x => x + 1);

function apply_func (f, x) {
  match (f) {
    when (Func(f)): f(x)
  }
}

function apply_func2<dom, codom> (f : func<dom, codom>, x) {
  match (f) {
    when (Func(f)): f(x)
  }
}

