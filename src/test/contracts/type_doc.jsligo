/** Doc for type */
export type t<a, b> =
  | #["A", int]
  | ["B", a]
  | #["C", b];

/**
 * Extracts `int` value from `t<int, int>`
 */
export function extract_int (v : t<int, int>): int {
  return match(v) {
    when (A(n)): n;
    when (B(n)): n;
    when (C(n)): n;
  }
}

